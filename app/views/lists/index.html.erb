<h1> To-Do List </h1>
<p><%= link_to 'New Item', new_list_path %></p>
<% @lists.each do |list| %>
  <h2><%= list.name %></h2>
  <h3> Status </h3>
  <%= list.status ? "Completed" : "Working on it..." %><br>
  <h3> Priority </h3>
  <%= list.priority %><br>
  <br>
  <%= link_to 'Show List', list_path(list) %>
  <%= link_to 'Edit List', edit_list_path(list) %>
<% end %>
